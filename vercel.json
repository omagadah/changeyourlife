{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [ { "type": "host", "value": "www.changeyourlife.ai" } ],
      "destination": "https://changeyourlife.ai/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/app", "destination": "/dashboard.html" },
    { "source": "/settings", "destination": "/settings.html" },
    { "source": "/profile", "destination": "/profile.html" },
    { "source": "/login", "destination": "/inscription.html" },
    { "source": "/signup", "destination": "/inscription.html" },
    { "source": "/journal", "destination": "/journal.html" }
  ]
}