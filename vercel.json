{
  "routes": [
    { "src": "^/$", "dest": "public/index.html" },
    { "src": "^/app$", "dest": "public/dashboard.html" },
    { "src": "^/settings$", "dest": "public/settings.html" },
    { "src": "^/profile$", "dest": "public/profile.html" },
    { "src": "^/login$", "dest": "public/inscription.html" },
    { "src": "^/signup$", "dest": "public/inscription.html" },
    { "src": "^/journal$", "dest": "public/journal.html" },
    { "src": "^/(.*)$", "dest": "public/$1" }
  ]
}